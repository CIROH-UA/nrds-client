{
  "name": "nrds_frontend",
  "version": "0.0.1",
  "description": "Frontend React project for Next Gen in a Box Visualizer",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config ./reactapp/config/webpack.config.js",
    "build": "webpack --mode production --config ./reactapp/config/webpack.config.js",
    "lint": "npx eslint reactapp",
    "test": "jest",
    "test:watch": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@react-aria/ssr": "^3.9.7",
    "@visx/axis": "^3.12.0",
    "@visx/event": "^3.12.0",
    "@visx/glyph": "^3.12.0",
    "@visx/grid": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/tooltip": "^3.12.0",
    "@visx/visx": "^3.12.0",
    "@visx/xychart": "^3.12.0",
    "@visx/zoom": "^3.12.0",
    "apache-arrow": "^21.1.0",
    "axios": "^0.30.2",
    "bootstrap": "^5.1.3",
    "color": "^4.2.3",
    "css-loader": "^6.5.1",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-time-format": "^4.1.0",
    "dotenv": "^16.0.1",
    "eslint-config-react-app": "^7.0.1",
    "file-loader": "^6.2.0",
    "maplibre-gl": "^4.7.1",
    "pmtiles": "^3.2.1",
    "prismjs": "^1.30.0",
    "proj4": "^2.20.2",
    "react": "^18.0.0",
    "react-bootstrap": "^2.4.0",
    "react-data-table-component": "^7.6.2",
    "react-dom": "^18.0.0",
    "react-icons": "^4.12.0",
    "react-map-gl": "^7.1.7",
    "react-router-bootstrap": "^0.26.1",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.8.0",
    "react-toastify": "^11.0.5",
    "react-window": "^1.8.10",
    "sass": "^1.49.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.5",
    "webpack": "^5.102.1",
    "webpack-dev-server": "^5.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-modules-commonjs": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@size-limit/preset-big-lib": "^11.1.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "browserslist": "^4.20.4",
    "compression-webpack-plugin": "^11.1.0",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.16.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-location-mock": "^1.0.9",
    "jest-watch-typeahead": "^1.1.0",
    "msw": "^2.12.1",
    "react-app-polyfill": "^3.0.0",
    "react-test-renderer": "^18.1.0",
    "size-limit": "^11.1.2",
    "terser-webpack-plugin": "^5.3.10",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
    ],
    "ignorePatterns": [
      "**/transforms/*.js",
      "**/config/*"
    ]
  },
  "size-limit": [
    {
      "path": "tethysapp/nrds/public/frontend/main.js",
      "limit": "10 kB"
    }
  ],
  "jest": {
    "roots": [
      "<rootDir>/reactapp"
    ],
    "collectCoverageFrom": [
      "reactapp/**/*.{js,jsx,ts,tsx}",
      "!reactapp/**/*.d.ts",
      "!reactapp/config/**/*"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/reactapp/config/tests/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/reactapp/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/reactapp/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/reactapp/config/tests/transforms/babelTransform.js",
      "^.+\\.css$": "<rootDir>/reactapp/config/tests/transforms/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/reactapp/config/tests/transforms/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!(ol|ol-mapbox-style|plotly.js|react-plotly.js)[/\\\\]).+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "modulePaths": [
      "<rootDir>/reactapp",
      "<rootDir>/node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactapp/config/tests/mocks/fileMock.js",
      "^.+\\.module\\.(css|sass|scss|less)$": "identity-obj-proxy",
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "babel-plugin-styled-components",
      [
        "prismjs",
        {
          "languages": "all",
          "theme": "tomorrow",
          "css": true
        }
      ]
    ]
  }
}
